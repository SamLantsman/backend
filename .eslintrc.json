{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
      
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "allow": ["_id", "_bar"]
    }
}
